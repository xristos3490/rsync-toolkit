{
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "extends": ["eslint:recommended", "plugin:node/recommended", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-process-exit": "off",
    "node/no-unsupported-features/node-builtins": [
      "error", {
        "version": ">=16.0.0",
        "ignores": []
      }
    ],
    "node/no-unsupported-features/es-syntax": [
      "error",
      {
        "version": ">=16.0.0",
        "ignores": ["modules"]
      }
    ]
  }
}
